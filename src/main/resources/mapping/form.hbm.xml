<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.sample.domain">

    <class name="Form" table="form">
        <id name="formId" column="formId"><generator class="increment"/></id>
        <property name="organizationForm" column="organizationForm"/>
    </class>

</hibernate-mapping>
